{
    "require": {
        "nikic/fast-route": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": ["config/settings.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml --do-not-cache-result --colors=always"
    }
}
